    <script>
      const urlParams = new URLSearchParams(window.location.search);
      const validation = urlParams.get("validation");
      const message = urlParams.get("message");
      const title = urlParams.get("title");
      const formId = urlParams.get("formId");
      console.log(window.location.pathname);
      urlParams.delete("validation");
      urlParams.delete("title");
      urlParams.delete("message");
      urlParams.delete("formId");
      console.log(formId);
      if (title === "Success") {
        displayAlert(title, message, 1);
      } else if (validation !== null && message !== null && title !== null) {
        displayAlert(title, message, 0);
      }

      var newUrl;

      if (urlParams.toString().length > 0)
        newUrl = `${window.location.pathname}?${urlParams.toString()}`;
      else newUrl = `${window.location.pathname}${urlParams.toString()}`;
      if (formId != null) newUrl += `#${formId}`;
      history.replaceState(null, "", newUrl);
    </script>
